{
  "name": "bots",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "pretest": "meteor npm run lint --silent",
    "test": "meteor test --settings settings.json --driver-package practicalmeteor:mocha",
    "test-app": "meteor test --settings settings.json --full-app --driver-package practicalmeteor:mocha",
    "start": "meteor run --settings settings.json"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true,
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "meteor",
      "babel",
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:meteor/recommended"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true,
      "jquery": true
    },
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {
      "react/jsx-uses-vars": 1,
      "react/jsx-uses-react": 1,
      "no-console": 0,
      "meteor/no-session": 0
    }
  },
  "dependencies": {
    "@elastic/datemath": "^2.3.0",
    "accounting": "^0.4.1",
    "aws-sdk": "^2.86.0",
    "babel-runtime": "^6.23.0",
    "bodybuilder": "^2.2.0",
    "bunyan": "^1.8.8",
    "chart.js": "^2.6.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "ddp": "^0.12.1",
    "ddp-login": "^1.0.10",
    "elasticsearch": "^12.1.3",
    "google-maps-api": "^2.0.1",
    "html2canvas": "^0.5.0-beta4",
    "http-aws-es": "^1.1.3",
    "immutable": "^3.8.1",
    "jquery": "^2.1.4",
    "lodash": "^4.17.4",
    "marker-clusterer-plus": "^2.1.4",
    "meteor-job": "^1.4.0",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "node-comparison": "^0.1.1",
    "node-rules": "^3.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.5",
    "react-markdown": "^2.5.0",
    "react-mounter": "^1.2.0",
    "react-paginate": "^4.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "string": "^3.3.3",
    "string-template": "^1.0.0",
    "tether": "^1.4.0",
    "uuid": "^3.0.1",
    "validate.js": "^0.11.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-jquery": "^1.0.1",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.10.0",
    "redux-devtools": "^3.4.0"
  }
}
